<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="users-file">



<insert id="insertFile" parameterType="hashmap" >

	insert into tb_file
	(
		 BOARD_IDX
		,ORIGINAL_FILE_NAME
		,STORED_FILE_NAME
		,FILE_SIZE
		,CREA_ID
	)
	VALUES
	(
		 #{BOARD_IDX}
		,#{ORIGINAL_FILE_NAME}
		,#{STORED_FILE_NAME}
		,#{FILE_SIZE}
		,'Admin'
	)

</insert>








</mapper>